<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sergiobetancourt.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/cejas"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="sergiobetancourt.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="sergiobetancourt.app.Calculadora"
            android:label="@string/title_activity_calculadora" >
        </activity>
        <activity
            android:name="sergiobetancourt.app.Conversor"
            android:label="@string/title_activity_conversor" >
        </activity>
    </application>

</manifest>
